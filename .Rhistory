picture <- function(img,side='right',width='100%') {
fullurl <- paste0('<img src=\"img/',img,'\" align=\"',side,'\" width=\"',width,'\" style=\"border:0.0px solid #eeeeee; padding:1px; margin:1px;\"/>',sep = "")
return(fullurl)
}
